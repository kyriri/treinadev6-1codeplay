<%# FIXME : field types %>

<%= form_for @instructor do |f| %>
  <%= f.label :name, 'Nome' %><br>
  <%= f.text_field :name %><br>

  <%= f.label :bio, 'Descrição' %><br>
  <%= f.text_area :bio %><br>

  <%= f.label :email, 'Email' %><br>
  <%= f.text_field :email %><br>


  <%= f.label :profile_picture, 'Foto de perfil' %><br>
  <%= f.text_field :profile_picture %><br>

  <%= f.submit 'Enviar' %>
<% end %>

<%= link_to 'Voltar', instructors_path %>
